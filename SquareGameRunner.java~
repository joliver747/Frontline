import javax.swing.JFrame;

public class SquareGameRunner extends JFrame{
  
  public Tetris(){
    add(new TetrisDriver());
    
    setDefaultCloseOperation(JFrame.EXIT_ON_CLOSE);
    setSize(230, 481);
    setLocationRelativeTo(null);
    setTitle("Tetris");
    
    setResizable(false);
    setVisible(true);
  }
  
  public static void main(String[] args){
    new Tetris();
  }
}